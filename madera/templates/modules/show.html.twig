{% extends 'base.html.twig' %}

{% block title %}Modules{% endblock %}

{% block body %}
    <h1>Modules</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ module.id }}</td>
            </tr>
            <tr>
                <th>Libelle_module</th>
                <td>{{ module.libelleModule }}</td>
            </tr>
            <tr>
                <th>Hauteur_module</th>
                <td>{{ module.hauteurModule }}</td>
            </tr>
            <tr>
                <th>Longueur_module</th>
                <td>{{ module.longueurModule }}</td>
            </tr>
            <tr>
                <th>Plan_coupe</th>
                <td>{{ module.planCoupe }}</td>
            </tr>
            <tr>
                <th>Parametre_prix</th>
                <td>{{ module.parametrePrix }}</td>
            </tr>
            <tr>
                <th>Cctp</th>
                <td>{{ module.cctp }}</td>
            </tr>
            <tr>
                <th>Is_modele</th>
                <td>{{ module.isModele ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Remplissage</th>
                <td>{{ module.Remplissage }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('modules_index') }}">back to list</a>

    <a href="{{ path('modules_edit', {'id': module.id}) }}">edit</a>

    {{ include('modules/_delete_form.html.twig') }}
{% endblock %}
